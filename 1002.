#include <stdio.h>
#include <string.h>
#include <stdlib.h>

char** commonChars(char** words, int wordsSize, int* returnSize) {
    int minFreq[26];
    for (int i = 0; i < 26; i++) minFreq[i] = 100; 
    for (int w = 0; w < wordsSize; w++) {
        int freq[26] = {0};
        for (int i = 0; words[w][i]; i++)
            freq[words[w][i] - 'a']++;
        for (int i = 0; i < 26; i++)
            if (freq[i] < minFreq[i])
                minFreq[i] = freq[i];
    }

    char** result = malloc(100 * sizeof(char*));
    *returnSize = 0;
    for (int i = 0; i < 26; i++) {
        while (minFreq[i]-- > 0) {
            result[*returnSize] = malloc(2);
            result[*returnSize][0] = 'a' + i;
            result[*returnSize][1] = '\0';
            (*returnSize)++;
        }
    }
    return result;
}
