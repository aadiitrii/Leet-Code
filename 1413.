#include <stdio.h>
int minStartValue(int* nums, int numsSize) {
    int total = 0;
    int minSum = 0;
    for (int i = 0; i < numsSize; i++) {
        total += nums[i];
        if (total < minSum) {
            minSum = total;
        }
    }
    return 1 - minSum;
}
int testMinStartValue() {
    int nums[] = {-3, 2, -3, 4, 2};
    int size = sizeof(nums) / sizeof(nums[0]);

    int result = minStartValue(nums, size);
    printf("Minimum start value: %d\n", result);

    return 0;
}
